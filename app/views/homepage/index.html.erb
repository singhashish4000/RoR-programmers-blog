<div class= "center jumbotron">
  <h1>PROGRAMER'S BLOG</h1>
  <% if !logged_in? %>
    <%= link_to "Sign up now", signup_path,class: "btn btn-primary btn-xlarge" %>
  <% else %>
  <div id='hello'>
  <%= link_to 'Hello,'+ current_user.username, user_path(current_user)  %>
  </div>    
  <% end %>
</div>